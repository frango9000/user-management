    "junit-report-merger": "4.0.0",
    "ng-packagr": "14.2.1",
    "npm-check-updates": "16.3.14",
    "nx": "15.0.0",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "sonarqube-scanner": "2.8.2",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "webpack-bundle-analyzer": "4.6.1"
  },
  "elf": {
    "cli": {
      "repoTemplate": "class",
      "inlineStoreInClass": false,
      "idKey": "id",
      "plugins": [
        "@ngneat/elf-cli-ng"
      ]
    }
  }
}
