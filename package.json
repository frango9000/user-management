    "@angular-devkit/schematics-cli": "14.2.6",
    "@angular-eslint/eslint-plugin": "14.1.2",
    "@angular-eslint/eslint-plugin-template": "14.1.2",
    "@angular-eslint/template-parser": "14.1.2",
    "@angular/cli": "14.2.6",
    "@angular/compiler-cli": "14.2.7",
    "@angular/language-service": "14.2.7",
    "@jest/reporters": "29.2.1",
    "@lensesio/cypress-websocket-testing": "1.0.1",
    "@ngneat/elf-cli": "3.0.1",
    "@ngneat/elf-cli-ng": "1.0.0",
    "@ngneat/transloco-keys-manager": "3.4.2",
    "@nrwl/cli": "15.0.0",
    "@nrwl/cypress": "15.0.0",
    "@nrwl/devkit": "15.0.0",
    "@nrwl/eslint-plugin-nx": "15.0.0",
    "@nrwl/jest": "15.0.0",
    "@nrwl/linter": "15.0.0",
    "@nrwl/node": "15.0.0",
    "@nrwl/workspace": "15.0.0",
    "@nxrocks/nx-spring-boot": "6.0.0",
    "@pact-foundation/pact": "9.17.3",
    "@schematics/angular": "14.2.6",
    "@schematics/schematics": "0.1102.12",
    "@types/jest": "28.1.8",
    "@types/jsonwebtoken": "8.5.9",
    "@types/node": "18.11.3",
    "@types/sockjs-client": "1.5.1",
    "@types/url-template": "2.0.28",
    "@typescript-eslint/eslint-plugin": "5.40.1",
    "@typescript-eslint/parser": "5.40.1",
    "cypress": "10.10.0",
    "dotenv": "16.0.3",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-github": "4.3.7",
    "jest": "28.1.3",
    "jest-environment-jsdom": "28.1.3",
    "jest-extended": "3.1.0",
    "jest-junit": "14.0.1",
    "jest-preset-angular": "12.2.2",
